    "react-dom": "^18.0.0",
    "react-query": "^3.34.19",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "cypress": "^9.5.3",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^7.0.4",
